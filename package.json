{
  "name": "@bifravst/app",
  "version": "0.0.0-development",
  "description": "Cat Tracker Web Application",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/app.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/app/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "MIT",
  "dependencies": {
    "@amcharts/amcharts4": "^4.5.14",
    "@bifravst/athena-helpers": "^3.0.0",
    "@bifravst/twemoji": "^12.1.2-typescript",
    "aws-amplify": "^1.1.36",
    "aws-amplify-react": "^2.3.12",
    "aws-iot-device-sdk": "^2.2.1",
    "aws-sdk": "^2.509.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "fast-deep-equal": "^2.0.1",
    "intro.js": "^2.9.3",
    "leaflet": "^1.5.1",
    "mcc-mnc-list": "git+https://github.com/bifravst/mcc-mnc-list.git#add-typescript-declarations",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-leaflet": "^2.4.0",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@bifravst/code-style": "^3.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@types/aws-iot-device-sdk": "^2.2.0",
    "@types/classnames": "^2.2.9",
    "@types/intro.js": "2.4.4",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-leaflet": "^2.4.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/reactstrap": "^8.0.1",
    "@types/uuid": "^3.4.5",
    "babel-eslint": "^10.0.2",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-app": "^6.0.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.3",
    "node-sass": "^4.12.0",
    "react-scripts": "^3.1.1",
    "ts-jest": "^24.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx",
      "git add"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
